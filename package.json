{
  "name": "react-router-mobx",
  "version": "1.0.0",
  "description": "when react-router meets mobx: observable router and location",
  "repository": "Cap32/react-router-mobx",
  "scripts": {
    "start": "npm test -- --watch",
    "test": "cross-env BABEL_ENV=cjs jest --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "rimraf": "rimraf ./dist ./lib ./es",
    "mkdir": "mkdirp ./dist ./lib ./es",
    "prebuild": "run-s rimraf mkdir",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src -d lib --ignore __tests__",
    "build:es": "cross-env BABEL_ENV=es babel src -d es --ignore __tests__",
    "build:umd": "cross-env NODE_ENV=production webpack",
    "build:min": "cross-env NODE_ENV=production webpack --env.minify",
    "build": "run-p build:*",
    "preversion": "npm test && npm run build"
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "lib",
    "es",
    "dist"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "author": "Cap32",
  "license": "MIT",
  "keywords": [
    "mobx",
    "react",
    "router",
    "react-router",
    "observable",
    "query",
    "location",
    "history",
    "querystring"
  ],
  "dependencies": {
    "prop-types": "^15.5.7",
    "tiny-querystring": "^0.0.2"
  },
  "peerDependencies": {
    "mobx": "^3.0.0",
    "react": "^0.14.0 || ^15.0.0",
    "react-router": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-system-import-transformer": "^2.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "coveralls": "^2.13.1",
    "cross-env": "^3.1.3",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.4",
    "lodash.camelcase": "^4.3.0",
    "mkdirp": "^0.5.1",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "npm-run-all": "^4.0.2",
    "qs": "^6.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.5.4",
    "webpack": "^3.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "modulePathIgnorePatterns": [
      "node_modules",
      "lib"
    ]
  }
}
